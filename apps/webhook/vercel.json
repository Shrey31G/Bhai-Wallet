{
  "version": 2,
  "buildCommand": "cd apps/webhook && tsc",
  "outputDirectory": "apps/webhook/dist",
  "builds": [
    {
      "src": "apps/webhook/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook/payment",
      "dest": "/apps/webhook/api/webhook/payment.ts"
    },
    {
      "src": "/api/health",
      "dest": "/apps/webhook/api/health.ts"
    }
  ]
}